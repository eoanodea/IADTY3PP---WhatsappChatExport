{
    "version": "3.0.0",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=webpack.config.js",
        "prod": "npm run production",
        "postinstall": "npm run prod",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=webpack.config.js",
        "echo-server": "laravel-echo-server start",
        "lev": "laravel-echo -v"
    },
    "devDependencies": {
        "axios": "^0.19",
        "bootstrap": "^4.4.1",
        "cross-env": "^7.0",
        "jquery": "^3.5",
        "laravel-mix": "^5.0.4",
        "lodash": "^4.17.15",
        "popper.js": "^1.16",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@carbon/charts": "^0.30.18",
        "@carbon/charts-vue": "^0.30.18",
        "@carbon/grid": "^10.10.2",
        "@carbon/themes": "^10.11.2",
        "@carbon/vue": "^2.24.1",
        "d3": "^5.16.0",
        "date-fns": "^2.12.0",
        "laravel-echo": "^1.7.0",
        "npm": "^6.14.4",
        "pusher-js": "^6.0.2",
        "url-loader": "^4.1.0",
        "vue-router": "^3.1.6",
        "vuex": "^3.3.0"
    }
